<Window x:Class="FirstProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FirstProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Adaugare" HorizontalAlignment="Center" Margin="0,74,0,0" VerticalAlignment="Top" Height="36" Width="136" Command="{Binding OpenWindowCommand}" CommandParameter="1"/>
        <Button Content="Modificare" HorizontalAlignment="Center" Margin="0,153,0,0" VerticalAlignment="Top" Height="41" Width="136" Command="{Binding OpenWindowCommand}" CommandParameter="2"/>
        <Button Content="Receptie" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="41" Width="136" Command="{Binding OpenWindowCommand}" CommandParameter="3"/>
        <Button Content="Vanzare" HorizontalAlignment="Center" Margin="0,295,0,0" VerticalAlignment="Top" Height="44" Width="136" Command="{Binding OpenWindowCommand}" CommandParameter="4"/>
        <Button Content="Generare Cod" HorizontalAlignment="Center" Margin="0,364,0,0" VerticalAlignment="Top" Height="39" Width="136" Command="{Binding OpenWindowCommand}" CommandParameter="5"/>


    </Grid>
</Window>
